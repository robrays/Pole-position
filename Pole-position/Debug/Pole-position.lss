                  .include "slotcar.inc"		
                 
                 ;***** Created: 2011-04-14 19:00 *****************************************
                 ;*************************************************************************
                 ;* Library for the SlotCar project
                 ;* 
                 ;* Author	     : RobTek gruppe 2
                 ;* File Name         : "slotcar.inc"
