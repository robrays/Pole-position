000223 e011      		LDI		temp, 0x01					
000224 e020      		LDI		temp2, 0x00	
                 						
                 		DIV:	
000225 0f55      			LSL		R21							
000226 1f22      			ROL		temp2						
000227 1724      			CP		temp2, R20					
000228 f418      			BRSH	subtr						
000229 0f11      			LSL		temp						
00022a f7d0      			BRCC	DIV							
00022b c005      			RJMP	RETURN						
                 											
                 		subtr:	
