                 //
                 
                 	UPDATE_DUTYCYCLE:
000206 931f      		PUSH	temp						; Push temp to Stack
000207 932f      		PUSH	temp2						; Push temp2 to Stack
                 
000208 e012      		LDI		temp, 2						; Multiply DCYCLE with 2
